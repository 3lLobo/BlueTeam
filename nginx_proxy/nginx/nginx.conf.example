map nvar_server_port nvar_target_port {
  default       80;
  5601          $KIBANA_PORT;
  9200          $ELASTICSEARCH_PORT;
}

stream {
  set nvar_minikubeip $MINIKUBE_IP;
  server {
    listen 80;
    proxy_pass nvar_minikubeip;
  }
  server {
    listen 5601;
    proxy_pass nvar_minikubeip:nvar_target_port;
  }
  server {
    listen 9200;
    proxy_pass nvar_minikubeip:nvar_target_port;
  }
}